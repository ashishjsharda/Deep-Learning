        "import numpy as np\n",
        "def leaky_relu(x,alpha=0.01):\n",
        "  return np.maximum(alpha*x,x)\n",
        "\n",
        "x=np.array([-1,0,1,2,3])\n",
        "print(leaky_relu(x))"
      ]
    }
